    <!-- NEW INSIGHTS DASHBOARD -->
    <div id="insights-view" class="view hidden">
      <style>
        /* Runtime overrides: prefer server-injected icon URLs, overriding any inlined base64 backgrounds */
        #insights-view .insights-menu-card[ data-team-icon="<?!= teamPerformanceIconDataUrl || '/assets/team-performance-icon.png' ?>"] {
          background-image: url('<?!= teamPerformanceIconDataUrl ?>') !important;
        }
        #insights-view .insights-menu-card[ data-offensive-icon="<?!= offensiveLeadersIconDataUrl || '/assets/offensive-leaders-icon.png' ?>"] {
          background-image: url('<?!= offensiveLeadersIconDataUrl ?>') !important;
        }
        #insights-view .insights-menu-card[ data-defensive-icon="<?!= defensiveWallIconDataUrl || '/assets/defensive-wall-icon.png' ?>"] {
          background-image: url('<?!= defensiveWallIconDataUrl ?>') !important;
        }
        #insights-view .insights-menu-card[aria-label="View player analysis"] .insights-menu-card-icon {
          background-image: url('<?!= playerAnalysisIconDataUrl ?>') !important;
        }
      </style>

      <script>
        // Set data-* attributes on the insights menu cards with server-injected fallback
        // This avoids direct edits to inline base64 background-image attributes and provides
        // a reliable client-side fallback for ensureInsightsCardImages() to pick up.
        (function () {
          var setIfNotPresent = function (selector, attrName, value) {
            try {
              var node = document.querySelector(selector);
              if (node && (!node.hasAttribute(attrName) || !node.getAttribute(attrName))) {
                node.setAttribute(attrName, value);
              }
            } catch (e) { /* defensive: ignore DOM errors */ }
          };
          setIfNotPresent('.insights-menu-card[ data-team-icon="<?!= teamPerformanceIconDataUrl || '/assets/team-performance-icon.png' ?>"]', 'data-team-icon', "<?!= teamPerformanceIconDataUrl || '/assets/team-performance-icon.png' ?>");
          setIfNotPresent('.insights-menu-card[ data-offensive-icon="<?!= offensiveLeadersIconDataUrl || '/assets/offensive-leaders-icon.png' ?>"]', 'data-offensive-icon', "<?!= offensiveLeadersIconDataUrl || '/assets/offensive-leaders-icon.png' ?>");
